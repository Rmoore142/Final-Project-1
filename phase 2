courses = []
personal_books = []

def add_course(course):
    """Add a course to the list."""
    courses.append(course)

def get_courses():
    """Return the list of courses."""
    return courses

def add_book(book):
    """Add a personal book to the list."""
    personal_books.append(book)

def get_personal_books():
    """Return the list of personal books."""
    return personal_books
